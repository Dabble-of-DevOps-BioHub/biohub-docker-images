{
  "docker_org": "dabbleofdevops",
  "project_slug": "biohub-docker-images",
  "pangeo_versions": ["2020.11.06"],
  "rstudio_version": "1.2.5042",
  "pangeo_notebook_version": "2020.11.06",
  "jupyterlab_version": "2.2.9",
  "pip_version": "20",
  "post_build": "jupyter labextension install --clean \\\n                            @kiteco/jupyterlab-kite \\\n                            @jupyter-widgets/jupyterlab-manager \\\n                            @jupyterlab/server-proxy \\\n                            dask-labextension\n",
  "default_packages": {
    "conda_packages": [
      "python=3.8*",
      "pangeo-notebook={{cookiecutter.pangeo_notebook_version}}",
      "jupyterlab={{cookiecutter.jupyterlab_version}}",
      "pip={{cookiecutter.pip_version}}",
      "jupyter-server-proxy",
      "jupyter_client",
      "lz4=3.1.1",
      "s3fs>0.5",
      "gcsfs",
      "fsspec",
      "numpy",
      "pandas",
      "xarray",
      "zarr",
      "hdf5",
      "pyarrow",
      "h5py",
      "h5netcdf"
    ],
    "pip_packages": [
      "jupyter-kite",
      "jupyter-rsession-proxy",
      "jupyter-book",
      "nbsphinx"
    ]
  }
}